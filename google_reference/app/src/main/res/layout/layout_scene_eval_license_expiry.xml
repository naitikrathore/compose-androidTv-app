<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">

    <TextView
        android:id="@+id/eval_licence_expiry_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingLeft="@dimen/custom_dim_290"
        android:paddingTop="@dimen/custom_dim_158_5"
        android:paddingRight="@dimen/custom_dim_290"
        android:paddingBottom="@dimen/custom_dim_4"
        android:textAlignment="center"
        android:textSize="@dimen/font_21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/eval_licence_expiry_message"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:letterSpacing="0.02"
        android:lineHeight="@dimen/font_21_9"
        android:lineSpacingExtra="@dimen/font_6_9"
        android:paddingLeft="@dimen/custom_dim_103_5"
        android:paddingRight="@dimen/custom_dim_103_5"
        android:textAlignment="center"
        android:textSize="@dimen/custom_dim_15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eval_licence_expiry_title" />

</androidx.constraintlayout.widget.ConstraintLayout>